<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<?import javafx.geometry.Insets?>
<BorderPane minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Application.Controller.Find.FindToDelete.FindProviderToDelete">
    <bottom>
        <HBox alignment="CENTER" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#handleCancel" prefWidth="150.0" text="Cancel">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </Button>
                <Button fx:id="confirmationButton" mnemonicParsing="false" onAction="#handleConfirmation"
                        prefWidth="150.0" text="Confirm">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </Button>
            </children>
        </HBox>
    </bottom>
    <center>
        <fx:include fx:id="findProviderSub" source="../../SubController/Find/FindProviderSub.fxml"/>
    </center>
</BorderPane>
